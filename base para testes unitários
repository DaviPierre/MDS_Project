// arquivo a ser testado (arquivo.js)
function multiplicar(a, b) {
  return a * b;
}

// arquivo de teste (arquivo.test.js)
const { multiplicar } = require('./arquivo');

test('multiplicar retorna o produto de dois números', () => {
  expect(multiplicar(2, 3)).toBe(6);
});

test('multiplicar retorna zero quando um dos parâmetros é zero', () => {
  expect(multiplicar(5, 0)).toBe(0);
  expect(multiplicar(0, 10)).toBe(0);
});
